@echo offrem Script will do as followed:rem - Check for PHP_COMMAND envrem - not found then detect phprem - else err and terminateif "%OS%"=="Windows_NT" @setlocalrem %~dp0 is expanded pathname of the current script under NTset DEFAULT_ORMAG_HOME=%~dp0..goto initgoto cleanup:initif "%ORMAG_HOME%" == "" set ORMAG_HOME=%DEFAULT_ORMAG_HOME%set DEFAULT_ORMAG_HOME=if "%PHP_COMMAND%" == "" goto no_phpcommandgoto rungoto cleanup:run"%PHP_COMMAND%" -d html_errors=off -qC "%ORMAG_HOME%\bin\ormag" %*goto cleanup:no_phpcommandrem PHP_COMMAND environment variable not found, assuming php.exe is on path.set PHP_COMMAND=php.exegoto init:err_homeecho ERROR: Environment var ORMAG_HOME not set. Please point thisecho variable to your local ormag installation!goto cleanup:cleanupif "%OS%"=="Windows_NT" @endlocalrem pause